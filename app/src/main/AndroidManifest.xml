<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.imac.daggerdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name=".simple.App"
        android:theme="@style/AppTheme">
        <activity android:name=".simple.main.MainActivity">

        </activity>
        <activity android:name=".simple.second.SecondActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".simple.simple.ThridActivity" />
        <activity android:name=".simple.simple.FourActivity" />
    </application>

</manifest>